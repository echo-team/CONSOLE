{
    "manifest_version": 2,
    "name": "CONSOLE",
    "version": "0.0.1",

    "browser_action":
    {
        "icon": "IMG/OPENNOACTIVE.PNG"
    },

    "background":
    {
        "scripts": ["JS/BACKGROUND/EVENTS.JS", "JS/BACKGROUND/CORE.JS", "JS/BACKGROUND/BUTTON.JS", "JS/BACKGROUND/MAIN.JS"]
    },
    "content_scripts":
    [
        {
            "matches": ["http://*/*", "https://*/*"],
            "js": ["JS/CONTENT/CONSOLE.JS", "JS/CONTENT/MAIN.JS"]
        }
    ],
    "permissions":
    [
        "tabs"
    ]
}
