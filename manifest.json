{
    "manifest_version": 2,
    "name": "CONSOLE",
    "version": "0.0.1",

    "browser_action":
    {
        "default_icon":
        {
            "16": "IMG/LOADING16.PNG",
            "24": "IMG/LOADING24.PNG",
            "32": "IMG/LOADING32.PNG"
        }
    },

    "background":
    {
        "scripts": ["JS/COMMON.JS", "JS/BACKGROUND/EVENTS.JS", "JS/BACKGROUND/CORE.JS", "JS/BACKGROUND/BUTTON.JS", "JS/BACKGROUND/MAIN.JS"]
    },
    "content_scripts":
    [
        {
            "run_at": "document_start",
            "matches": ["<all_urls>"],
            "css": ["CSS/CONTENT/COMMON.CSS", "CSS/CONTENT/RISE.MIN.CSS", "CSS/CONTENT/THEMES/NEWSPAPER.CSS", "CSS/CONTENT/INTERFACE.CSS", "CSS/CONTENT/SEPARATOR.CSS"],
            "js": ["JS/CONTENT/BASIS.MIN.JS", "JS/CONTENT/RISE.MIN.JS", "JS/CONTENT/INTERFACE.JS", "JS/CONTENT/PAGE.JS", "JS/CONTENT/CONSOLE.JS", "JS/CONTENT/SEPARATOR.JS", "JS/CONTENT/MAIN.JS"]
        }
    ],
    "permissions": ["tabs", "webNavigation", "webRequest", "webRequestBlocking", "*://*/*"],
    "web_accessible_resources": ["HTML/GHOST.HTML"]
}
