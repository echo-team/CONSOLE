{
    "manifest_version": 2,
    "name": "CONSOLE",
    "version": "0.0.1",

    "browser_action":
    {
        "icon": "IMG/OPENNOACTIVE.PNG"
    },

    "background":
    {
        "scripts": ["JS/BACKGROUND/EVENTS.JS", "JS/BACKGROUND/CORE.JS", "JS/BACKGROUND/BUTTON.JS", "JS/BACKGROUND/MAIN.JS"]
    },
    "content_scripts":
    [
        {
            "run_at": "document_start",
            "matches": ["<all_urls>"],
            "js": ["JS/CONTENT/MAIN.JS"]
        }
    ],
    "permissions": ["tabs", "webNavigation", "webRequest", "webRequestBlocking", "*://*/*"],
    "web_accessible_resources": ["HTML/GHOST.HTML"]
}
