{
    "manifest_version": 2,
    "name": "CONSOLE",
    "version": "0.0.1",

    "browser_action":
    {
        "icon": "IMG/OPENNOACTIVE.PNG"
    },

    "background":
    {
        "scripts": ["JS/BACKGROUND/EVENTS.JS", "JS/BACKGROUND/CORE.JS", "JS/BACKGROUND/BUTTON.JS", "JS/BACKGROUND/MAIN.JS"]
    },
    "content_scripts":
    [
        {
            "matches": ["http://*/*", "https://*/*", "file:///*"],
            "css": ["CSS/CONTENT/RISE.MIN.CSS", "CSS/CONTENT/THEMES/NEWSPAPER.CSS", "CSS/CONTENT/INTERFACE.CSS", "CSS/CONTENT/CONSOLE.CSS"],
            "js": ["JS/CONTENT/BASIS.MIN.JS", "JS/CONTENT/RISE.MIN.JS", "JS/CONTENT/CONSOLE.JS", "JS/CONTENT/INTERFACE.JS", "JS/CONTENT/MAIN.JS"]
        }
    ],
    "permissions": ["tabs", "webRequest", "webRequestBlocking", "*://*/*"]
}
